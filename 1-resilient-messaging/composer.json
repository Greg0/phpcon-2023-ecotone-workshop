{
    "type": "workshop",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\App\\": "tests"
        }
    },
    "require": {
        "php": "^8.2",
        "doctrine/orm": "^2.16",
        "ecotone/lite-amqp-starter": "^1.0.1",
        "ecotone/lite-dbal-starter": "1.0.1",
        "ecotone/open-telemetry": "^1.204",
        "open-telemetry/exporter-otlp": "^1.0",
        "open-telemetry/transport-grpc": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
